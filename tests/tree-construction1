| <x> (, x, )
|   foo:bar="" (foo, bar, )

#data
<x:z xmlns:x="y" xml:foo:bar="f"></x:z>
#errors
#document
| <x:z> (x, z, y)
|   xmlns:x="y" (xmlns, x, http://www.w3.org/2000/xmlns/)
|   xml:foo:bar="f" (xml, foo:bar, http://www.w3.org/XML/1998/namespace)

#data
<xml:foobar/>
#errors
#document
| <xml:foobar> (xml, foobar, http://www.w3.org/XML/1998/namespace)

